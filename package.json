{
  "name": "karachuonyo-website",
  "version": "1.0.0",
  "description": "Karachuonyo First Political Campaign Website",
  "main": "index.html",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "dev": "npm run build-css",
    "build": "npm run build-css-prod"
  },
  "keywords": [
    "political",
    "campaign",
    "website",
    "karachuonyo"
  ],
  "author": "Karachuonyo First Campaign",
  "license": "MIT",
  "dependencies": {
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/aspect-ratio": "^0.4.2"
  }
}